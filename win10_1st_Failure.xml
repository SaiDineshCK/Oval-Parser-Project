	  <definitions>
      <definition id="oval:mil.disa.stig.windows:def:129" version="1" class="compliance">
	<metadata>
		<title>Built-in Admin Account Status</title>
		<affected family="windows">
			<platform>Microsoft Windows 10</platform>
		</affected>
		<description>The built-in administrator account must be disabled.</description>
		<reference source="http://cce.mitre.org" ref_id="CCE-43470-4"/>
	</metadata>
	<criteria operator="AND">
		<criterion test_ref="oval:mil.disa.stig.windows:tst:12900" comment="Verifies 'Accounts: Administrator account status' is set to 'Disabled'"/>
	</criteria>
</definition>
  </definitions>
  <tests>
  <user_sid55_test xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows" id="oval:mil.disa.stig.windows:tst:12900" version="2" comment="'Accounts: Administrator account status' is set to 'Disabled'" check="at least one">
	<object object_ref="oval:mil.disa.stig.windows:obj:12900"/>
	<state state_ref="oval:mil.disa.stig.windows:ste:12900"/>
</user_sid55_test>
  </tests>
  <states>
    <user_sid55_state xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows" id="oval:mil.disa.stig.windows:ste:12900" version="1" comment="Enabled equals variable">
	<enabled datatype="boolean" var_ref="oval:mil.disa.stig.windows:var:12900"/>
</user_sid55_state>
  </states>
  <objects>
  <wmi57_object xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows" id="oval:mil.disa.stig.windows:obj:12901" version="4" comment="Local Built-in Administrator">
	<namespace datatype="string">root\cimv2</namespace>
	<wql datatype="string" operation="equals">SELECT SID FROM Win32_UserAccount WHERE LocalAccount=True AND SID LIKE 'S-1-5-21-%-500'</wql>
</wmi57_object>
     <user_sid55_object xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows" id="oval:mil.disa.stig.windows:obj:12900" version="2" comment="Built-in Administrator account">
	<user_sid var_ref="oval:mil.disa.stig.windows:var:12901" datatype="string"/>
</user_sid55_object>
  </objects>
     <variables>
	 <local_variable id="oval:mil.disa.stig.windows:var:12901" version="3" datatype="string" comment="Built-in Administrator SID">
	<object_component object_ref="oval:mil.disa.stig.windows:obj:12901" item_field="result" record_field="sid"/>
</local_variable>
    <external_variable id="oval:mil.disa.stig.windows:var:12900" version="3" datatype="boolean" comment="Status of Admin Account">
          <possible_value hint="disabled">0</possible_value>
          <possible_value hint="enabled">1</possible_value>
        </external_variable>
		   </variables>
